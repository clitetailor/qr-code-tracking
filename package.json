    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "prettier-plugin-svelte": "^0.6.0",
    "rollup": "^1.2.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^4.2.3",
