{
  "name": "qr-code-tracking",
  "version": "1.0.0",
  "scripts": {
    "server:start": "node server/index.js",
    "server:dev": "nodemon --watch server -q server/index.js",
    "web:build": "rollup -c rollup.config.js",
    "web:dev": "run-p web:start web:autobuild",
    "web:autobuild": "rollup -c rollup.config.js -w",
    "web:start": "sirv public --dev",
    "database:migrate": "sequelize db:migrate",
    "prettier": "prettier --config .prettierrc.yaml --write web/**/*.js --write server/**/*.js"
  },
  "dependencies": {
    "@koa/cors": "2",
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "apollo-server": "^2.4.8",
    "apollo-server-koa": "^2.4.8",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "graphql": "^14.2.0",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "material-design-lite": "^1.3.0",
    "moment": "^2.24.0",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "qrcode": "^1.3.3",
    "sequelize": "^5.2.5",
    "svelte": "^3.0.0-beta.20",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "dotenv": "^7.0.0",
    "graphql-tag": "^2.10.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.2.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "sequelize-cli": "^5.4.0",
    "sirv-cli": "^0.2.3"
  }
}
